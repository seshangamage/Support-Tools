<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="playground.wso2telco.sheshan.com.wso2playgroundapp">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".StartMenu">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ProductiontMenu" >
        </activity>
        <activity android:name=".OperatorsMenu" >
        </activity>
        <activity android:name=".ProductionTokenRequest" >
        </activity>
        <activity
            android:name=".UnregisterNumberRequest"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name=".ProductionWebView"
            android:theme="@android:style/Theme.NoTitleBar" />
    </application>

</manifest>